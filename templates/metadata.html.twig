{% if metatags %}
	{% for Metatag in metatags %}
		<meta name="{{Metatag.getKey()}}" content="{{Metatag.getValue()|raw}}" />
	{% endfor %}
	{% for Metatag in systemMetatags %}
		{% if Metatag.getKey() == 'link:icon' %}
			<link rel="icon" type="image/x-icon" href="{{Metatag.getValue()|raw}}" />
		{% elseif Metatag.getKey() == 'Cache-Control' %}
			<meta http-equiv="Cache-Control" content="{{Metatag.getValue()|raw}}" />
		{% elseif Metatag.getKey() == 'link:apple-touch-icon' %}
			<link rel="apple-touch-icon" href="{{Metatag.getValue()|raw}}" />
		{% elseif Metatag.getKey() == 'charset' %}
			<meta charset="{{Metatag.getValue()|raw}}" />
		{% else %}
			<meta name="{{Metatag.getKey()}}" content="{{Metatag.getValue()|raw}}" />
		{% endif %}
	{% endfor %}
{% endif %}