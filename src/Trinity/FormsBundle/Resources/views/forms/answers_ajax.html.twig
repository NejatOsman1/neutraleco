<div class="card">
	<div class="card-content">
		<div class="table-responsive">
			<table class="table">
				<tr>
					<td>Datum:</td>
					<td>{{Answer.session.dateStart|date('d-m-Y H:i:s')}}</td>
				</tr>
				{% for key, value in Answer.answer %}
					<tr>
						<td>{{key}}:</td>
						{% if value is iterable %}
						<td>
						    <ul>
						        {% for v in value %}
						        <li>{{v}}</li>
						        {% endfor %}
						    </ul>
						</td>
						{% else %}
						<td>{{value}}</td>
						{% endif %}
					</tr>
				{% endfor %}
			</table>
		</div>
	</div>
</div>
