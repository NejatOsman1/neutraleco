<form method="post">
	{# {% extends '@Cms/base.html.twig' %}
	{% trans_default_domain 'cms' %}
	{% block body %} #}
		<div class="row">
			<div class="col s12">
				<div class="card">
					<div class="card-content">
						<div class="card-title">{{'Selecteer een versie'|trans}}</div>
						<div class="input-field ">
							<select id="form_robots" name="version">
								{% for i,version in version_list %}
									<option {% if Client.version == version.name %}selected="selected"{% endif %} value="{{version.name}}">{{version.name}}{% if Client.version == version.name %} - Huidige versie{% elseif i == 0 %} - Meest recente versie{% endif %}</option>
								{% endfor %}
							</select>
							<label for="form_robots" class="required">{{ 'Versie' | trans }}</label>
						</div>

						<div class="input-field">
							<input id="email" type="email" name="email" class="validate" placeholder="{{Settings.adminEmail}}" />
							<label for="email">{{ 'Notificatie e-mailadres (optioneel)' | trans }}</label>
						</div>
					</div>
				</div>
			</div>
		</div>
		<div id="popup-footer">
			<button type="submit" class="btn green">{{ 'Updaten' | trans }}</button>
		</div>
	{# {% endblock %} #}
</form>